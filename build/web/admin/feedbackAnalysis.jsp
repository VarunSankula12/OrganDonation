                        <div class="midde_cont">
                            <div class="container-fluid">
                                <%
                                    int five=0;
                                    int four=0;
                                    int three=0;
                                    int two=0;
                                    int one=0;
                                    String query="SELECT * FROM feedback";
                                    ResultSet rs=adminDao.Retrieve(query);
                                    while(rs.next()){
                                        if(rs.getString("rating").equals("5")){
                                            five++;
                                        }
                                        if(rs.getString("rating").equals("4")){
                                            four++;
                                        }
                                        if(rs.getString("rating").equals("3")){
                                            three++;
                                        }
                                        if(rs.getString("rating").equals("2")){
                                            two++;
                                        }
                                        if(rs.getString("rating").equals("1")){
                                            one++;
                                        }
                                    }
                                %>
                                <!-- row -->
                                <div class="row column1">
                                    <div class="container-fluid">
                                        <div class="row justify-content-center">
                                            <div class="col-6">
                                                <canvas id="myChart" width="800" height="500"></canvas>
                                            </div>
                                        </div>
                                        <script>
                                            const ctx = document.getElementById('myChart').getContext('2d');
                                            const myChart = new Chart(ctx, {
                                                type: 'bar',
                                                data: {
                                                    labels: ['Very Positive', 'Positive', 'Neutral', 'Negative', 'Very Negative'],
                                                    datasets: [{
                                                        label: ' Reviews',
                                                        data: [<%=five%>, <%=four%>, <%=three%>,<%=two%>, <%=one%>],
                                                        backgroundColor: [
                                                            'rgba(255, 99, 132, 0.5)',
                                                            'rgba(54, 162, 235, 0.5)',
                                                            'rgba(255, 206, 86, 0.5)',
                                                            'rgba(75, 192, 192, 0.5)',
                                                            'rgba(153, 102, 255, 0.5)',
                                                        ],
                                                        borderColor: [
                                                            'rgba(255, 99, 132, 1)',
                                                            'rgba(54, 162, 235, 1)',
                                                            'rgba(255, 206, 86, 1)',
                                                            'rgba(75, 192, 192, 1)',
                                                            'rgba(153, 102, 255, 1)',
                                                        ],
                                                        borderWidth: 1
                                                    }]
                                                },
                                                options: {
                                                    scales: {
                                                        y: {
                                                            beginAtZero: true
                                                        }
                                                    }
                                                }
                                            });
                                        </script>
                                    </div>
                                </div>
                            </div>
